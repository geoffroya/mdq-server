<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    default-lazy-init="true"
    xmlns:c="http://www.springframework.org/schema/c"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:util="http://www.springframework.org/schema/util"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

    <!--
        *********************************************************
        ***                                                   ***
        ***   I M P O R T   E N T I T Y   B L A C K L I S T   ***
        ***                                                   ***
        *********************************************************
    -->

    <!--
        Entities listed in the set below will be discarded from the eduGAIN channel.

        Because the normal behaviour is to silently discard entities which fail to
        pass the checking rulesets, entities are only added to this set when the basic
        processing fails to resolve a problem.
    -->
    <util:set id="int_edugain_entity_blacklist">

    </util:set>

</beans>
